{
  "name": "vega-loader",
  "version": "3.1.0",
  "description": "Network request and file loading utilities.",
  "keywords": [
    "vega",
    "loader",
    "file",
    "http",
    "fetch",
    "json",
    "csv",
    "tsv",
    "topojson",
    "parse",
    "type",
    "inference"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "main": "build/vega-loader.js",
  "module": "index",
  "repository": "vega/vega",
  "scripts": {
    "build": "yarn test && terser build/vega-loader.js -c -m -o build/vega-loader.min.js",
    "pretest": "rm -rf build && mkdir build&& rollup -f umd -g d3-dsv:d3,d3-time-format:d3,vega-util:vega,topojson-client:topojson -n vega -o build/vega-loader.js -- index.js",
    "test": "tape 'test/**/*-test.js' && eslint index.js src test",
    "prepublishOnly": "yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-dsv": "^1.0.10",
    "d3-time-format": "^2.1.3",
    "node-fetch": "^2.3.0",
    "topojson-client": "^3.0.0",
    "vega-util": "^1.7.0"
  },
  "browser": {
    "buffer": false,
    "fs": false,
    "node-fetch": false
  }
}
