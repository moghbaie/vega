{
  "name": "vega-dataflow",
  "version": "4.1.0",
  "description": "Reactive dataflow processing.",
  "keywords": [
    "vega",
    "dataflow",
    "reactive",
    "streaming",
    "data"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "main": "build/vega-dataflow.js",
  "module": "index",
  "repository": "vega/vega",
  "scripts": {
    "build": "yarn test && terser build/vega-dataflow.js -c -m -o build/vega-dataflow.min.js",
    "pretest": "rm -rf build && mkdir build && rollup -g vega-loader:vega,vega-util:vega -f umd -n vega -o build/vega-dataflow.js -- index.js",
    "test": "tape 'test/**/*-test.js' && eslint index.js src test",
    "prepublishOnly": "yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-loader": "^3.1.0",
    "vega-util": "^1.7.0"
  }
}
